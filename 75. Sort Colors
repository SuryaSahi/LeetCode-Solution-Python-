        #nums have be interchanged with s
        
        zero = 0
        one = 0
        two = 0
        for i in range(len(s)):
            if s[i] == 0:
                zero +=1
            elif s[i] == 1:
                one +=1
            else:
                two +=1
        i = 0
        while ( zero != 0):
            s[i] = 0
            i+=1
            zero -=1
        while ( one != 0):
            s[i] = 1
            i+=1
            one -=1
        while ( two != 0):
            s[i] = 2
            i+=1
            two -=1
        return s     
        
